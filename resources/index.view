      <g transform="rotate(36)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(42)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(48)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(54)"><rect class="mainDialMinute markerColor"/></g>
