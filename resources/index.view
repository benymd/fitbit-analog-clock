<svg id="clock" viewport-fill="black">
  <!-- 336x336 -->
  <defs>
    <symbol id="mainDial">
      <g transform="rotate(0)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(6)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(12)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(18)"><rect class="mainDialMinute markerColor"/></g>
      <g transform="rotate(24)"><rect class="mainDialMinute markerColor"/></g>
